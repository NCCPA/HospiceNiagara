@model IEnumerable<HospiceNiagara.Models.ViewModels.MyHospiceViewModel>

<div class="page-header">
    <h1>My Hospice</h1>
</div>

<!-- Nav tabs -->
<div role="tabpanel">

    <ul class="nav nav-tabs" role="tablist">
        <li role="presentation" class="active"><a href="#announcementsTab" aria-controls="announcementsTab" role="tab" data-toggle="tab">Announcements</a></li>        
        <li role="presentation"><a href="#meetingsTab" aria-controls="meetingsTab" role="tab" data-toggle="tab">Meetings</a></li>
        <li role="presentation"><a href="#deathsTab" aria-controls="deathsTab" role="tab" data-toggle="tab">Death Notice</a></li>
    </ul>
    <br />

    <div class="row">

        <!-- Tab panes -->
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="announcementsTab">@Html.Action("_announcement","announcement")</div>            
            <div role="tabpanel" class="tab-pane" id="meetingsTab">@Html.Partial("_MeetingControl")</div>
            <div role="tabpanel" class="tab-pane" id="deathsTab">@Html.Partial("_dnControl")</div>
        </div>

    </div>
</div>
